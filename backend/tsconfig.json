{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2016",
    "sourceMap": true,
    "noImplicitAny":true,
    "strictNullChecks":true,
    "allowJs":true,
    "resolveJsonModule":true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "./src/**/*"
  ]
}